{
    "name": "workload-token-contract-bnb",
    "version": "1.0.0",
    "description": "工作量证明代币智能合约 - BNB链版本",
    "main": "index.js",
    "scripts": {
        "compile": "npx hardhat compile",
        "test": "npx hardhat test",
        "deploy:bnb-testnet": "npx hardhat run scripts/deploy-bnb.js --network bscTestnet",
        "deploy:bnb-mainnet": "npx hardhat run scripts/deploy-bnb.js --network bscMainnet",
        "verify:bnb-testnet": "npx hardhat verify --network bscTestnet",
        "verify:bnb-mainnet": "npx hardhat verify --network bscMainnet",
        "gas-report": "REPORT_GAS=true npx hardhat test"
    },
    "dependencies": {
        "@openzeppelin/contracts": "^4.9.3",
        "hardhat": "^2.17.1",
        "web3": "^4.3.0"
    },
    "devDependencies": {
        "@nomicfoundation/hardhat-toolbox": "^3.0.0",
        "@nomiclabs/hardhat-ethers": "^2.2.3",
        "ethers": "^5.7.2"
    },
    "keywords": [
        "solidity",
        "ethereum",
        "bsc",
        "bnb",
        "erc20",
        "workload",
        "proof-of-work",
        "binance-smart-chain"
    ],
    "author": "AINO Team",
    "license": "MIT"
}